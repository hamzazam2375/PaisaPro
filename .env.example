# Example .env for PaisaPro (do NOT commit real secrets)
# Copy this to .env and fill secrets for local development

# Django secret key (override the checked-in fallback for production)
DJANGO_SECRET_KEY=your-secret-key-here

# Email (Gmail) credentials for WorkingEmailBackend
EMAIL_HOST_USER=youremail@gmail.com
EMAIL_HOST_PASSWORD=your-email-app-password
EMAIL_USE_TLS=True
EMAIL_PORT=587

# Google Gemini API key for chatbot features
GEMINI_API_KEY=your-gemini-api-key-here

# PostgreSQL Database Configuration
DB_NAME=paisapro_db
DB_USER=postgres
DB_PASSWORD=your-postgres-password
DB_HOST=localhost
DB_PORT=5432
